<Window x:Class="RenjuAdvisor.View.CutBoardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:local="clr-namespace:RenjuAdvisor.View"
        mc:Ignorable="d"
        DataContext="{Binding Source={StaticResource Locator}, Path=CutBoard}"
        Title="{Binding Path=CutBoardArgs.WzqGameProcess.MainWindowTitle}" Height="450" Width="800" >
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction Command="{Binding ViewLoadedCommnad}"/>
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <Grid>
        
    </Grid>
</Window>
